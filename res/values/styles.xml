<?xml version="1.0" encoding="utf-8"?>

<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="Theme.Travis_custom" parent="@style/Theme.Sherlock">
        <item name="actionBarItemBackground">@drawable/selectable_background_travis_custom</item>
        <item name="popupMenuStyle">@style/PopupMenu.Travis_custom</item>
        <item name="dropDownListViewStyle">@style/DropDownListView.Travis_custom</item>
        <item name="actionBarTabStyle">@style/ActionBarTabStyle.Travis_custom</item>
        <item name="actionDropDownStyle">@style/DropDownNav.Travis_custom</item>
        <item name="actionBarStyle">@style/ActionBar.Transparent.Travis_custom</item>
        <item name="actionModeBackground">@drawable/cab_background_top_travis_custom</item>
        <item name="actionModeSplitBackground">@drawable/cab_background_bottom_travis_custom</item>
        <item name="actionModeCloseButtonStyle">@style/ActionButton.CloseMode.Travis_custom</item>


    </style>

    <style name="ActionBar.Solid.Travis_custom" parent="@style/Widget.Sherlock.ActionBar.Solid">
        <item name="background">@drawable/ab_solid_travis_custom</item>
        <item name="backgroundStacked">@drawable/ab_stacked_solid_travis_custom</item>
        <item name="backgroundSplit">@drawable/ab_bottom_solid_travis_custom</item>
        <item name="progressBarStyle">@style/ProgressBar.Travis_custom</item>
    </style>

    <style name="ActionBar.Transparent.Travis_custom" parent="@style/Widget.Sherlock.ActionBar">
        <item name="background">@drawable/ab_transparent_travis_custom</item>
        <item name="progressBarStyle">@style/ProgressBar.Travis_custom</item>
    </style>

    <style name="PopupMenu.Travis_custom" parent="@style/Widget.Sherlock.ListPopupWindow">
        <item name="android:popupBackground">@drawable/menu_dropdown_panel_travis_custom</item>
    </style>

    <style name="DropDownListView.Travis_custom" parent="@style/Widget.Sherlock.ListView.DropDown">
        <item name="android:listSelector">@drawable/selectable_background_travis_custom</item>
    </style>

    <style name="ActionBarTabStyle.Travis_custom" parent="@style/Widget.Sherlock.ActionBar.TabView">
        <item name="android:background">@drawable/tab_indicator_ab_travis_custom</item>
    </style>

    <style name="DropDownNav.Travis_custom" parent="@style/Widget.Sherlock.Spinner.DropDown.ActionBar">
        <item name="android:background">@drawable/spinner_background_ab_travis_custom</item>
        <item name="android:popupBackground">@drawable/menu_dropdown_panel_travis_custom</item>
        <item name="android:dropDownSelector">@drawable/selectable_background_travis_custom</item>
    </style>

    <style name="ProgressBar.Travis_custom" parent="@style/Widget.Sherlock.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/progress_horizontal_travis_custom</item>
    </style>

    <style name="ActionButton.CloseMode.Travis_custom" parent="@style/Widget.Sherlock.ActionButton.CloseMode">
        <item name="android:background">@drawable/btn_cab_done_travis_custom</item>
    </style>

    <!-- this style is only referenced in a Light.DarkActionBar based theme -->
    <style name="Theme.Travis_custom.Widget" parent="@style/Theme.Sherlock">
        <item name="popupMenuStyle">@style/PopupMenu.Travis_custom</item>
        <item name="dropDownListViewStyle">@style/DropDownListView.Travis_custom</item>
    </style>

    <style name="RatingBartravis_custom" parent="android:Widget.RatingBar">
      <item name="android:progressDrawable">@drawable/travis_custom_ratingbar_full_holo_dark</item>
      <item name="android:indeterminateDrawable">@drawable/travis_custom_ratingbar_full_holo_dark</item>
  </style>

  <style name="RatingBarBigtravis_custom">
      <item name="android:progressDrawable">@drawable/travis_custom_ratingbar_holo_dark</item>
      <item name="android:indeterminateDrawable">@drawable/travis_custom_ratingbar_holo_dark</item>
      <item name="android:minHeight">35dip</item>
      <item name="android:maxHeight">35dip</item>
  </style>

  <style name="RatingBarSmalltravis_custom">
      <item name="android:progressDrawable">@drawable/travis_custom_ratingbar_small_holo_dark</item>
      <item name="android:indeterminateDrawable">@drawable/travis_custom_ratingbar_small_holo_dark</item>
      <item name="android:minHeight">16dip</item>
      <item name="android:maxHeight">16dip</item>
  </style>

</resources>